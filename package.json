{
  "name": "base-react-typescript-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development react-app-rewired start",
    "start:prod": "cross-env NODE_ENV=production react-app-rewired start",
    "build:dev": "cross-env NODE_ENV=development react-app-rewired build",
    "build:prod": "cross-env NODE_ENV=production react-app-rewired build",
    "eject": "react-app-rewired eject",
    "lint": "eslint ./src --ext .tsx --ext .ts",
    "lint:fix": "eslint --fix ./src --ext .tsx --ext .ts",
    "open:test:server": "http-server ./build/ -p 8075 -c-1 -a localhost --ssl --key ./certs/test_key.pem --cert ./certs/test_cert.pem",
    "test:run": "cypress run --headless --browser chrome",
    "test": "cross-env START_SERVER_AND_TEST_INSECURE=1 server-test 'yarn open:test:server' 'https://localhost:8075' 'yarn test:run'",
    "test:dev": "cypress open"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "classnames": "^2.2.6",
    "i18next": "^19.8.5",
    "localforage": "^1.9.0",
    "lodash": "^4.17.20",
    "query-string": "^6.13.8",
    "react": "^17.0.1",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.5",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "==3.4.4",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "seamless-immutable": "^7.1.4",
    "typescript": "^4.1.3",
    "use-debounce": "^5.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cypress": "^1.1.3",
    "@types/lodash": "^4.14.168",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/seamless-immutable": "^7.1.15",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "cypress": "^6.3.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "http-server": "^0.12.3",
    "node-sass": "==4.14.1",
    "react-app-rewire-alias": "^0.2.0",
    "start-server-and-test": "^1.11.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
