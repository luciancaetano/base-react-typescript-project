    "baseUrl": ".",
    "paths": {
      "@feature/*": [
        "./src/app/features/*"
      ],
      "@components/*": [
        "./src/app/components/*"
      ],
      "@config/*": [
        "./src/app/config/*"
      ],
      "@hooks/*": [
        "./src/app/hooks/*"
